{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Move",
    "description": "Coordinates To Play The Game",
    "type": "object",
    "properties": {
        "x": {
            "description": "X Coordinate To Play",
            "type": "integer",
            "Minimum": 0,
            "Maximum":9
        },
        "y": {
            "description": "Y Coordinate To Play",
            "type": "integer",
            "Minimum": 0,
            "Maximum":9
        }
    },
    "required": ["x", "y"]
}